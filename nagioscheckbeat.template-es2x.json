{
  "mappings": {
    "_default_": {
      "_all": {
        "norms": {
          "enabled": false
        }
      },
      "dynamic_templates": [
        {
          "fields": {
            "mapping": {
              "ignore_above": 1024,
              "index": "not_analyzed",
              "type": "string"
            },
            "match_mapping_type": "string",
            "path_match": "fields.*"
          }
        }
      ],
      "properties": {
        "@timestamp": {
          "type": "date"
        },
        "beat": {
          "properties": {
            "hostname": {
              "ignore_above": 1024,
              "index": "not_analyzed",
              "type": "string"
            },
            "name": {
              "ignore_above": 1024,
              "index": "not_analyzed",
              "type": "string"
            }
          }
        },
        "critical": {
          "type": "double"
        },
        "label": {
          "ignore_above": 1024,
          "index": "not_analyzed",
          "type": "string"
        },
        "max": {
          "type": "double"
        },
        "message": {
          "index": "analyzed",
          "norms": {
            "enabled": false
          },
          "type": "string"
        },
        "min": {
          "type": "double"
        },
        "name": {
          "ignore_above": 1024,
          "index": "not_analyzed",
          "type": "string"
        },
        "status": {
          "ignore_above": 1024,
          "index": "not_analyzed",
          "type": "string"
        },
        "status_code": {
          "type": "long"
        },
        "tags": {
          "ignore_above": 1024,
          "index": "not_analyzed",
          "type": "string"
        },
        "took_ms": {
          "type": "long"
        },
        "uom": {
          "ignore_above": 1024,
          "index": "not_analyzed",
          "type": "string"
        },
        "value": {
          "type": "double"
        },
        "warning": {
          "type": "double"
        }
      }
    }
  },
  "order": 0,
  "settings": {
    "index.refresh_interval": "5s"
  },
  "template": "nagioscheckbeat-*"
}