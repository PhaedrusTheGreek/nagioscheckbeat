{
   "template": "nagioscheckbeat-*",
   "settings": {
      "index.refresh_interval": "5s",
      "number_of_shards": 1,
      "number_of_replicas": 1
   },
   "mappings": {
      "nagiosmetric": {
         "_all": {
            "enabled": false
         },
         "dynamic_templates": [
            {
               "string_fields": {
                  "match": "*",
                  "match_mapping_type": "string",
                  "mapping": {
                     "type": "string",
                     "index": "not_analyzed",
                     "doc_values": true
                  }
               }
            }
         ],
         "properties": {
            "@timestamp": {
               "type": "date",
               "doc_values": true
            },
            "value": {
               "type": "double",
               "doc_values": true
            },
            "min": {
               "type": "double",
               "doc_values": true
            },
            "max": {
               "type": "double",
               "doc_values": true
            },
            "critical": {
               "type": "double",
               "doc_values": true
            },
            "warning": {
               "type": "double",
               "doc_values": true
            }
         }
      },
      "nagioscheck": {
         "_all": {
            "enabled": true,
            "omit_norms": true
         },
         "dynamic_templates": [
            {
               "string_fields": {
                  "match": "*",
                  "match_mapping_type": "string",
                  "mapping": {
                     "type": "string",
                     "index": "not_analyzed",
                     "doc_values": true
                  }
               }
            }
         ],
         "properties": {
            "@timestamp": {
               "type": "date",
               "doc_values": true
            },
            "message": {
               "type": "string",
               "index": "analyzed",
               "omit_norms": true,
               "fielddata": {
                  "format": "disabled"
               }
            }
         }
      }
   }
}
